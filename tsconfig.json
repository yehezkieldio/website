{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "#/*": ["./src/*"]
        },
        "strictNullChecks": true,
        "allowJs": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "noUnusedLocals": false,
        "noUnusedParameters": false
    }
}
